{
    "name": "ragezbla/zfc-twitter-bootstrap",
    "description": "A module that assists in implementing Twitter Bootstrap for ZF2/3",
    "type": "library",
    "keywords": [
        "zf2",
        "twBootstrap",
        "Twitter Bootstrap"
    ],
    "homepage": "https://github.com/mwillbanks/ZfcTwitterBootstrap",
    "authors": [
        {
            "name": "Mike Willbanks",
            "email": "mike@digitalstruct.com",
            "homepage": "http://blog.digitalstruct.com/"
        },
        {
            "name": "Bart Riepe",
            "email": "aeolun@gmail.com",
            "homepage": "https://serial-experiments.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laminas/laminas-filter": "2.* || 3.*",
        "laminas/laminas-form": "2.* || 3.*",
        "laminas/laminas-navigation": "2.* || 3.*",
        "laminas/laminas-view": "2.* || 3.*",
        "laminas/laminas-mvc-plugin-flashmessenger": "*",
        "laminas/laminas-dependency-plugin": "^2.0",
        "laminas/laminas-i18n": "2.* || 3.*"
    },
    "require-dev": {
        "laminas/laminas-mvc-skeleton": "dev-master",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-0": {
            "ZfcTwitterBootstrap": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "replace": {
        "mwillbanks/zfc-twitter-bootstrap": "*"
    }
}
